node{
stage('SCM Checkout'){
git 'https://github.com/lazycoder95/simple-java-maven-app'
}
stage('Compile'){
def mvnhome = tool name: 'maven-3', type: 'maven'
sh "${mvnhome}/bin/mvn package"
}

stage('deploy to tom')
{
sshagent(['sshtest']) {
    // some block
    sh 'scp -o StrictHostKeyChecking=no target/*.war deva@192.168.68.2:/opt/tomcat/webapps'
}
}

stage('slack notificatio')
{
slackSend baseUrl: 'https://hooks.slack.com/services/', 
channel: '#jenkins-demo', color: 'good', message: 'HO', teamDomain: 'TeamDemo', 
tokenCredentialId: 'SLACK-DEMO'}
}
